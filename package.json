{
  "name": "date-pickle",
  "version": "0.0.1",
  "description": "A framework-agnostic library to manage calendars",
  "main": "src/index.ts",
  "scripts": {
    "start": "npx nodemon",
    "exec": "npx ts-node ./src/index.ts",
    "build": "rimraf ./dist && tsc -p tsconfig.build.json",
    "test": "cross-env NODE_ICU_DATA=node_modules/full-icu jest",
    "test:watch": "cross-env NODE_ICU_DATA=node_modules/full-icu jest --watch"
  },
  "keywords": [
    "typescript",
    "calendar"
  ],
  "author": "Alain D'Ettorre <alain.det@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^28.1.8",
    "@types/node": "^18.7.14",
    "cross-env": "^7.0.3",
    "full-icu": "^1.5.0",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  }
}
